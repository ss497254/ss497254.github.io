{
    "name": "ss497254-portfolio",
    "description": "My protfolio Website",
    "version": "1.0.0",
    "scripts": {
        "build": "gatsby build",
        "develop": "gatsby develop -H 0.0.0.0 -p 3000",
        "deploy": "gatsby build --prefix-paths && gh-pages -d public",
        "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
        "start": "npm run develop",
        "serve": "gatsby serve -H 0.0.0.0 -p 3000",
        "clean": "gatsby clean",
        "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
    },
    "dependencies": {
        "bootstrap": "^4.5.0",
        "gatsby": "^4.14.0",
        "gatsby-image": "^3.11.0",
        "gatsby-plugin-manifest": "^4.14.0",
        "gatsby-plugin-react-helmet": "^5.14.0",
        "gatsby-plugin-sass": "^5.14.0",
        "gatsby-plugin-sharp": "^4.14.1",
        "gatsby-source-filesystem": "^4.14.0",
        "gatsby-transformer-sharp": "^4.14.0",
        "node-sass": "^4.14.1",
        "react": "^17.0.2",
        "react-bootstrap": "^1.0.1",
        "react-dom": "^17.0.2",
        "react-helmet": "^6.1.0",
        "react-reveal": "^1.2.2",
        "react-scroll": "^1.8.7",
        "react-tilt": "^0.1.4"
    },
    "devDependencies": {
        "babel-eslint": "^10.1.0",
        "eslint": "^7.2.0",
        "eslint-config-airbnb": "^18.2.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.21.2",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.20.0",
        "eslint-plugin-react-hooks": "^4.0.4",
        "gh-pages": "^3.2.3",
        "husky": "^4.2.5",
        "lint-staged": "^10.2.11",
        "prettier": "^2.0.5"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ss497254/ss497254"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.+(js|jsx)": [
            "eslint --fix",
            "git add"
        ],
        "*.+(json|css|md)": [
            "prettier --write",
            "git add"
        ]
    }
}
